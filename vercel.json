{

    "builds": [
        {
            "src":"./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "source":"/(.*)",
            "dest":"/"
        }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "access-control-allow-origin",
              "value": "*"
            }
          ]
        }
      ]
}


  